os: windows
and app.name: chrome.exe
os: windows
and app.exe: chrome.exe
-
continue:
    key(f8)

step over:
    key(f10)

step into:
    key(f11)

step out:
    key(shift-f11)

toggle breakpoint:
    key(ctrl-b)

show console:
    key(esc)

select file:
    key(ctrl-p)

select anything:
    key(ctrl-o)

select panels:
    key(ctrl-shift-p)
