app: windows_explorer
app: windows_file_browser
-

up directory:
    key(alt-left)

down directory:
    key(alt-right)

go address:
    key(alt-d)

address history:
    key(alt-down)

go search pane:
    key(alt-d)
    key(tab)
    repeat(1)

go folder pane:
    key(alt-d)
    key(tab)
    repeat(2)

go file pane:
    key(alt-d)
    key(tab)
    repeat(3)