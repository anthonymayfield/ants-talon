app: windows_explorer
app: windows_file_browser
-

up directory:
    key(alt-up)

down directory:
    key(alt-right)

go address:
    key(alt-d)

address history:
    key(alt-down)

go search pane:
    key(ctrl-e)

go folder pane:
    key(ctrl-e)
    key(tab:1)

go file pane:
    key(ctrl-e)
    key(tab:2)